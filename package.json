{
  "name": "builder-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--no-node-snapshot --trace-deprecation' next dev",
    "build": "cross-env NODE_OPTIONS='--no-node-snapshot' next build",
    "start": "cross-env NODE_OPTIONS='--no-node-snapshot' next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:xo": "xo"
  },
  "dependencies": {
    "@builder.io/sdk-react-nextjs": "^0.23.0",
    "@sentry/nextjs": "^10.0.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "next": "16.0.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.4.0",
    "@tailwindcss/postcss": "^4.0.8",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "cross-env": "^10.0.0",
    "daisyui": "^5.0.0-beta.8",
    "eslint": "^9.0.0",
    "eslint-config-next": "16.0.8",
    "eslint-plugin-unicorn": "^62.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^4.0.8",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0",
    "xo": "^1.2.3"
  },
  "xo": {
    "ignores": [
      "__tests__/**",
      "jest.config.ts",
      "jest.setup.tsx",
      "commitlint.config.js",
      "eslint.config.mjs"
    ]
  },
  "overrides": {
    "@eslint/plugin-kit": "^0.3.4"
  }
}
