{
  "name": "builder-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--no-node-snapshot --trace-deprecation' next dev",
    "build": "cross-env NODE_OPTIONS='--no-node-snapshot' next build",
    "start": "cross-env NODE_OPTIONS='--no-node-snapshot' next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:xo": "xo",
    "prepare": "husky",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.14.0",
    "@builder.io/admin-sdk": "^0.0.8",
    "@builder.io/sdk-react-nextjs": "^0.23.0",
    "@sentry/nextjs": "^10.0.0",
    "@tailwindcss/typography": "^0.5.19",
    "@vercel/analytics": "^1.6.1",
    "@vercel/postgres": "^0.10.0",
    "@vercel/speed-insights": "^1.3.1",
    "drizzle-orm": "^0.45.1",
    "next": "16.0.10",
    "postgres": "^3.4.7",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "resend": "^6.6.0"
  },
  "devDependencies": {
    "@builder.io/cli": "^1.2.10",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "cross-env": "^10.0.0",
    "daisyui": "^5.5.13",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.0.10",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^4.0.8",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.0.0",
    "xo": "^1.2.3"
  },
  "xo": {
    "ignores": [
      "__tests__/**",
      "jest.config.ts",
      "jest.setup.tsx",
      "commitlint.config.js",
      "eslint.config.mjs",
      "scripts/**",
      "middleware.ts",
      "app/components/**",
      "app/api/**",
      "app/activiteiten/**",
      "app/nieuws/**",
      "app/lib/**",
      "app/(admin)/**",
      "app/features/admin/**"
    ]
  },
  "overrides": {
    "@eslint/plugin-kit": "^0.5.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.0.0"
  }
}
